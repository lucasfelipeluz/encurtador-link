{
  "name": "encurtador-link",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "nodemon 'src/' --exec 'ts-node -r tsconfig-paths/register src/index.ts' -e ts",
    "create-migration": "npx sequelize-cli migration:create --migrations-path 'src/infrastructure/migrations' --name",
    "push-migrations": "npm run build && ts-node src/infrastructure/_sequelize/createDatabase.ts && npx sequelize-cli db:migrate",
    "undo-migrations": "npx sequelize-cli db:migrate:undo:all"
  },
  "author": "Lucas Luz @lucasfelipeluz",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.5",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "mysql2": "^3.11.5",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsyringe": "^4.8.0",
    "typescript": "5.7.2"
  }
}
